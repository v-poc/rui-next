{
  "name": "rui-next",
  "version": "0.49.4",
  "description": "Mobile web UI components based on React hooks and Vite 8",
  "files": [
    "dist/rui-next.css",
    "dist/rui-next.es.js",
    "dist/rui-next.umd.js",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/rui-next.umd.js",
  "module": "dist/rui-next.es.js",
