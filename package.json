  "devDependencies": {
    "@rsbuild/plugin-less": "^1.6.0",
    "@rspress/plugin-preview": "2.0.0-beta.4",
    "@types/node": "^22.19.7",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "classnames": "^2.5.1",
    "less": "^4.5.1",
    "rspress": "2.0.0-beta.4",
    "typescript": "^5.9.3",
    "vite": "^8.0.0-beta.11"
  }
