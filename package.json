  "devDependencies": {
    "@rspress/plugin-preview": "^1.40.0",
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "classnames": "^2.5.1",
    "less": "^4.2.1",
    "rspress": "^1.40.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.7"
  }
